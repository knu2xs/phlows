{
  "routes": [
    {
      "route": "/phlows/*.{css,js,ico,json}",
      "serve": "/phlows/$1"
    },
    {
      "route": "/phlows/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
